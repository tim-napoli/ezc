program fibonacci_suit

function fibonacci(in n is integer) return integer
begin
    on n < 2 do return 1
    return fibonacci(n - 2) + fibonacci(n - 1)
end

function fibonacci_suit(in args is vector of string) return integer
    local n is integer
    local result is integer
begin
    if args.size() < 2 then
        print "usage: fibonacci n\n"
        return 1
    endif

    n = atoi(args.at(0))
    result = fibonacci(n)

    print "fibonacci({n}) = {result}"
end

