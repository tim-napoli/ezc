program fibonacci

function fibonacci(in n is integer) return integer
begin
    on n < 2 do return 1
    return fibonacci(n - 2) + fibonacci(n - 1)
end

function main(in args is vector of string) return integer
    local n is integer
    local result is integer
begin
    if args.size() < 2 then
    begin
        print "usage: fibonacci n\n"
        return 1
    end

    n = atoi(args[0])
    result = fibonacci(n)

    print "fibonacci({n}) = {result}"
end

