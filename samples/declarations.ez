program person

structure Person is
  name is string
  age is integer
end

global n is integer
global p is string
global v is vector of boolean
global vv is vector of vector of natural
global vvvv is vector of vector of vector of vector of real
global p is Person
global persons is vector of Person

constant PI         is real    = 3
constant TOTO       is string  = "toto"
constant MAX_PRIMES is integer = 20
constant EXPR       is boolean = 4 * PI * 6 + 3 * 7 <= MAX_PRIMES * 2 + 1 * 5
